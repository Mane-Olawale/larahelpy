    "license": "MIT",
    "homepage": "https://github.com/mane-olawale/laratweaky",
    "support": {
        "issues": "https://github.com/mane-olawale/laratweaky/issues",
        "source": "https://github.com/mane-olawale/laratweaky"
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ilesanmi Olawale Adedotun",
